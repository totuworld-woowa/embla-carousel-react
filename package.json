  },
  "homepage": "https://github.com/davidcetinkaya/embla-carousel-react#readme",
  "files": [
    "lib/**/*"
  ],
  "browserslist": "last 5 versions",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "webpack -p --config ./webpack/webpack.config",
