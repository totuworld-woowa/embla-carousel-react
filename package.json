    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^25.4.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
